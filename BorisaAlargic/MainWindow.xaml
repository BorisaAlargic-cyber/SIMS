<Window x:Class="BorisaAlargic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BorisaAlargic"
        xmlns:toolBar="clr-namespace:UI.Components.Toolbar.View;assembly=UI"
        xmlns:contentView="clr-namespace:UI.Components.Content.View;assembly=UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="500">
    <DockPanel LastChildFill="True">
        <toolBar:ToolbarView x:Name="toolbar" DockPanel.Dock="Top"/>
        <contentView:ContentView x:Name="content"/>
    </DockPanel>
</Window>
